# Discord Bot Configuration
DISCORD_TOKEN=your_bot_token_here
CLIENT_ID=your_bot_client_id_here

# Honeypot Role IDs (comma-separated)
HONEYPOT_ROLES=1234567890123456789,1234567890123456790,1234567890123456791

# Honeypot Channel IDs (comma-separated)  
HONEYPOT_CHANNELS=1234567890123456792,1234567890123456793

# Role Timeout Duration (in milliseconds)
# Default: 86400000 = 24 hours
# 3600000 = 1 hour, 43200000 = 12 hours, 604800000 = 7 days
DEFAULT_ROLE_TIMEOUT=86400000

# Optional: Logging Channel ID (uncomment the logging sections in code to use)
# LOG_CHANNEL_ID=your_log_channel_id_here